	<params>
		<param name="toolbar" type="list" default="Default" label="Functionality" description="Select functionality">
			<option value="Default">Advanced</option>
			<option value="Basic">Basic</option>
		</param>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="text_direction" type="list" default="ltr" label="Text Direction" description="Ability to change text direction">
 			<option value="ltr">Left to Right</option>
 			<option value="rtl">Right to Left</option>
 		</param>
		<param name="content_css" type="radio" default="0" label="Template CSS classes" description="Load CSS classes from template_css.css">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
 		<param name="content_css_custom" type="text" default="" label="Custom CSS Classes" description="You can specify the loading of a custom CSS file - simply enter the FULL path to the css file you want loaded"/>
		<param name="newlines" type="list" default="false" label="Newlines" description="Newlines will be made into the selected option">
 			<option value="false">P Elements</option>
			<option value="true">BR Elements</option>
 		</param>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="docprops" type="radio" default="false" label="Doc-props" description="Show/Hide docprops buttons">
 			<option value="false">Unenable</option>
 			<option value="true">Enable</option>
 		</param>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="wwidth" type="text" default="100%" label="HTML Width" description="Width of HTML mode pop-up window. Only works in `Advanced` mode" />
		<param name="hheight" type="text" default="350" label="HTML Height" description="Height of HTML mode pop-up window. Only works in `Advanced` mode" />
	</params>
