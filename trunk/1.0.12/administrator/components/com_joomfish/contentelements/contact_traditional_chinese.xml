<?xml version="1.0" encoding="UTF-8"?>
<joomfish type="contentelement">
  <name>連絡人</name>
  <author>A. Kempkens</author>
  <version>1.7</version>
  <description>定義核心程式的連絡人元件</description>
  <reference>
  	<table name="contact_details">
  		<field type="referenceid" name="id" translate="0">ID</field>
  		<field type="titletext" name="name" translate="1">名稱</field>
  		<field type="text" name="con_position" translate="1">職稱</field>
  		<field type="text" name="address" translate="1">住址</field>
  		<field type="text" name="suburb" translate="1">市鎮</field>
  		<field type="text" name="state" translate="1">州省</field>
  		<field type="text" name="country" translate="1">國家</field>
  		<field type="text" name="postcode" translate="1">郵遞區號</field>
  		<field type="text" name="telephone" translate="1">電話</field>
  		<field type="text" name="fax" translate="1">傳真</field>
  		<field type="textarea" name="misc" translate="1">其它資訊</field>
  		<field type="textarea" name="params" translate="1">參數值</field>
  		<field type="checked_out_by" name="checked_out" translate="0">檢視人</field>
  		<field type="checked_out_date" name="checked_out_time" translate="0">檢視日期</field>
  	</table>
  </reference>
  <translationfilters>
  	  <category>catid</category>
  </translationfilters>  
</joomfish>