<?xml version="1.0" ?>
<joomfish type="contentelement">
  <name>Mosets Tree Custom Fields</name>
  <author>Ivo Apostolov</author>
  <version>2.2</version>
  <description>Definition for the mosets tree custom fields</description>
  <reference type="mt_customfields">
  	<table name="mt_customfields">
  		<field type="referenceid" name="cf_id" translate="0">ID</field>
  		<field type="titletext" name="caption"  length="50" maxlength="250" translate="1">Caption</field>
  		<field type="text" name="default_value"  length="50" maxlength="250" translate="1">Default Value</field>
  		<field type="textarea" name="field_elements"  length="50" maxlength="250" translate="1">Field Elements</field>
  		<field type="textarea" name="prefix_text_mod"  length="50" maxlength="250" translate="1">Prefix Text</field>
 		<field type="textarea" name="suffix_text_mod"  length="50" maxlength="250" translate="1">Sufix Text</field>
 		<field type="text name="prefix_text_display"  length="50" maxlength="250" translate="1">Prefix Text</field>
 		<field type="text" name="suffix_text_display"  length="50" maxlength="250" translate="1">Sufix Text Display</field>
 		<field type="text" name="search_caption"  length="50" maxlength="250" translate="1">Search Caption</field>
 		<field type="text" name="params"  length="50" maxlength="250" translate="1">Params</field>
		
		
	</table>
  </reference>
</joomfish>
