<?xml version="1.0" ?>
<joomfish type="contentelement">
  <name>Mosets Tree Link Check</name>
  <author>Ivo Apostolov</author>
  <version>2.2</version>
  <description>Definition for the mosets tree link check</description>
  <reference type="mt_linkcheck">
  	<table name="mt_linkcheck">
  		<field type="referenceid" name="img_id" translate="0">ID</field>
  		<field type="titletext" name="link_name"  length="50" maxlength="250" translate="1">Link Name</field>
  		<field type="text" name="field"  length="50" maxlength="250" translate="1">Field</field>
  		<field type="text" name="domain"  length="50" maxlength="250" translate="1">Domain</field>
  		<field type="text" name="path"  length="50" maxlength="250" translate="1">Path</field>
	</table>
  </reference>
</joomfish>
