<?xml version="1.0" ?>
<joomfish type="contentelement">
  <name>DOCman:documents</name>
  <author>Crispin Lee & Jozsef Tamas Herczeg</author>
  <version>1.5</version>
  <description>DOCman Documents Definition for Joom!Fish 1.8</description>
  <reference>
  	<table name="docman">
  		<field type="referenceid" name="id" translate="0">ID</field>
  		<field type="int" name="catid" translate="0">Catid</field>
    	<field type="titletext" name="dmname" translate="1">Name</field>
  		<field type="htmltext" name="dmdescription" translate="1">Description</field>
      <field type="created_date" name="dmdate_published" translate="0">Date</field>
      <field type="modified_date" name="dmlastupdateon" translate="0">Updared</field>
      <field type="checked_out_date" name="checked_out_time" translate="0">Checked out</field>
	  </table>
  </reference>
  <translationfilters>
  	  <keyword>dmname</keyword>
  </translationfilters>
</joomfish>
