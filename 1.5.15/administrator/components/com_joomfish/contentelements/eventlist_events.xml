<?xml version="1.0" ?>
<joomfish type="contentelement">
  <name>EventList Events</name>
  <author>Christoph Lukes</author>
  <version>1.0</version>
  <description>Definition for EventList Events</description>
  <reference>
	<table name="eventlist_events">
		<field type="referenceid" name="id" translate="0">ID</field>
		<field type="titletext" name="title" translate="1">Title</field>
		<field type="text" name="alias" translate="1">Alias</field>
		<field type="htmltext" name="datdescription" translate="1">Description</field>
		<field type="textarea" name="meta_description" translate="1">Meta Description</field>
		<field type="text" name="datimage" translate="0">Image</field>
		<field type="checked_out_by" name="checked_out" translate="0">Checked out by</field>
		<field type="checked_out_date" name="checked_out_time" translate="0">Checked out date</field>
	</table>
  </reference>
  <translationfilters>
  	  <id>id</id>
  	  <keyword>title</keyword>
  </translationfilters>
</joomfish>
