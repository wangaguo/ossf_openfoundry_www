	<authorEmail>extensions@tekdi.net</authorEmail>
	<authorUrl>http://www.tekdi.net</authorUrl>
	<version>1.0.1</version>
	<description>
		This plugin will show events submitted by the user in their profile. 
	</description>
	<files>
		<filename plugin="eventlist_cb">eventlist_cb.php</filename>
		<filename>index.html</filename>
		<filename>eventlist_cb.css</filename>
		<filename>language/english.php</filename>
	</files>
	<tabs>
		<tab name="My Events" description="Eventlist Plugin" class="eventlistTab" fields="0" position="cb_tabmain" displaytype="tab">
		<params>
			<param name="end_date" type="radio" default="0" label="End Date" description="Show/Hide the end date ">
				<option value="0">Hide</option>
				<option value="1">Show</option>
        	</param>
			<param name="start_date" type="radio" default="0" label="Start Date" description="Show/Hide the start date ">
				<option value="0">Hide</option>
				<option value="1">Show</option>
        	</param>
		</params>
			<fields>
			</fields>
		</tab>
	</tabs>
	<install>
		<queries>
		</queries>
	</install>
	<uninstall>
	</uninstall>
</cbinstall>

